/*
 * Example XPR syntax
 */

// type declarations
type Buyer;
type AuctionID Int;
type Winner (AuctionID, Buyer);

// shared sets
declare Set<Winner> winnerSet;
declare Set<Buyer> buyerSet;

// example invariant
assert Forall AuctionID auction, Buyer buyer: 
    Winner(auction, buyer) in winnerSet => buyer in buyerSet;